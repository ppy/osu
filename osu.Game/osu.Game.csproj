    <Title>osu!</Title>
    <PackageId>ppy.osu.Game</PackageId>
    <Version>0.0.0</Version>
    <PackageIcon>icon.png</PackageIcon>
    <IsPackable>true</IsPackable>
  </PropertyGroup>
  <ItemGroup Label="Icon">
